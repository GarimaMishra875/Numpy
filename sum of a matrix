import numpy as np
a=[]
b=[]
s1=int(input('enter the size : '))
print('enter the elements :')
for i in range(0,s1):
    e1=int(input())
    a.append(e1)
s2=int(input('enter the size : '))
print('enter the elements :')
for i in range(0,s2):
    e2=int(input())
    b.append(e2)
arr=np.array([a,b])
print(arr)
print(np.sum(arr))
